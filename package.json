{
  "name": "sw2ngx",
  "version": "6.5.1",
  "description": "auto-generation of angular services by Swagger / OpenAPI declaration",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "author": "https://github.com/YAZART",
  "license": "WTFPL",
  "homepage": "https://github.com/ngxutils/sw2ngx#readme",
  "bugs": {
    "url": "https://github.com/ngxutils/sw2ngx/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngxutils/sw2ngx.git"
  },
  "keywords": [
    "swagger",
    "openapi",
    "angular",
    "javascript",
    "generator",
    "ngx"
  ],
  "scripts": {
    "build": "rm -rf ./build && run-p build:* && npm run mkdir:templates && npm run copy",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "mkdir:templates": "mkdir ./build/templates",
    "copy": "cp -R ./src/templates/* ./build/templates/default",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "test": "run-s build test:*",
    "test:lint": "eslint src --ext .ts"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "change-case": "^4.1.2",
    "ejs": "^3.1.6",
    "node-fetch": "^2.6.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.7",
    "tsyringe": "^4.5.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.0.7",
    "@types/node": "^14.14.37",
    "@types/node-fetch": "^2.5.10",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "files": [
    "bin",
    "build/**/*",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "sw2ngx/README.md",
    "config.scheme.json"
  ],
  "prettier": {
    "singleQuote": true
  },
  "bin": {
    "sw2ngx": "./bin/sw2ngx"
  }
}
